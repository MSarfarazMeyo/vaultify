{
  "expo": {
    "name": "Vaultify",
    "slug": "vaultify",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "vaultify",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "usesAppleSignIn": true,
      "bundleIdentifier": "com.vaultify.app",
      "buildNumber": "18",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Your permission allows us to access your media gallery so you can upload images from your photo library."
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      [
        "expo-image-picker",
        {
          "photosPermission": "Your permission allows us to access your media gallery so you can upload images from your photo library ."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "dfe286c3-3dc5-4199-bae3-1b32fc16854f"
      }
    }
  }
}
